<?xml version="1.0"?>
<h:html xmlns:h="http://www.w3.org/1999/xhtml" xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:jr="http://openrosa.org/javarosa" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Fill This In</h:title>
    <model>
      <instance>
        <faims id="Fill_This_In">
          <style>
            <orientation>
              <orientation/>
            </orientation>
            <even>
              <layout_weight/>
            </even>
            <large>
              <layout_weight/>
            </large>
          </style>
          <Tabgroup>
            <Tab>
              <Dummy/>
              <Checkbox/>
              <Dropdown/>
              <List/>
              <Picture/>
              <Radio/>
            </Tab>
          </Tabgroup>
        </faims>
      </instance>
    </model>
  </h:head>
  <h:body>
    <group ref="style">
      <label/>
      <group ref="orientation">
        <label/>
        <input ref="orientation">
          <label>horizontal</label>
        </input>
      </group>
      <group ref="even">
        <label/>
        <input ref="layout_weight">
          <label>1</label>
        </input>
      </group>
      <group ref="large">
        <label/>
        <input ref="layout_weight">
          <label>3</label>
        </input>
      </group>
    </group>
    <group ref="Tabgroup" faims_archent_type="Tabgroup">
      <label>{Tabgroup}</label>
      <group ref="Tab">
        <label>{Tab}</label>
        <input faims_attribute_type="measure" ref="Dummy" faims_attribute_name="Dummy" faims_style_class="required">
          <label>{Dummy}</label>
        </input>
        <select faims_attribute_type="vocab" ref="Checkbox" faims_attribute_name="Checkbox" faims_style_class="required">
          <label>{Checkbox}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select>
        <select1 faims_attribute_type="vocab" ref="Dropdown" faims_attribute_name="Dropdown" faims_style_class="required">
          <label>{Dropdown}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select1>
        <select1 appearance="compact" ref="List" faims_attribute_name="List" faims_style_class="required">
          <label>{List}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select1>
        <select1 type="image" faims_attribute_type="vocab" ref="Picture" faims_attribute_name="Picture" faims_style_class="required">
          <label>{Picture}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select1>
        <select1 appearance="full" faims_attribute_type="vocab" ref="Radio" faims_attribute_name="Radio" faims_style_class="required">
          <label>{Radio}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select1>
      </group>
    </group>
  </h:body>
</h:html>
