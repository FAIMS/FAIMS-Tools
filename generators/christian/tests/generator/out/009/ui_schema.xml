<?xml version="1.0"?>
<h:html xmlns:h="http://www.w3.org/1999/xhtml" xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:jr="http://openrosa.org/javarosa" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Fill This In</h:title>
    <model>
      <instance>
        <faims id="Fill_This_In">
          <style>
            <orientation>
              <orientation/>
            </orientation>
            <even>
              <layout_weight/>
            </even>
            <large>
              <layout_weight/>
            </large>
          </style>
          <Tabgroup>
            <Tab>
              <Dummy/>
              <Dropdown_1/>
              <List_1/>
              <Dropdown_2/>
              <List_2/>
            </Tab>
          </Tabgroup>
          <Tabgroup_Other>
            <Tab>
              <Dummy/>
            </Tab>
          </Tabgroup_Other>
        </faims>
      </instance>
    </model>
  </h:head>
  <h:body>
    <group ref="style">
      <label/>
      <group ref="orientation">
        <label/>
        <input ref="orientation">
          <label>horizontal</label>
        </input>
      </group>
      <group ref="even">
        <label/>
        <input ref="layout_weight">
          <label>1</label>
        </input>
      </group>
      <group ref="large">
        <label/>
        <input ref="layout_weight">
          <label>3</label>
        </input>
      </group>
    </group>
    <group ref="Tabgroup" faims_archent_type="Tabgroup">
      <label>{Tabgroup}</label>
      <group ref="Tab">
        <label>{Tab}</label>
        <input faims_attribute_type="measure" ref="Dummy" faims_attribute_name="Dummy">
          <label>{Dummy}</label>
        </input>
        <select1 ref="Dropdown_1" faims_annotation="false" faims_certainty="false">
          <label>{Dropdown_1}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select1>
        <select1 appearance="compact" ref="List_1" faims_annotation="false" faims_certainty="false">
          <label>{List_1}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select1>
        <select1 ref="Dropdown_2" faims_annotation="false" faims_certainty="false">
          <label>{Dropdown_2}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select1>
        <select1 appearance="compact" ref="List_2" faims_annotation="false" faims_certainty="false">
          <label>{List_2}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select1>
      </group>
    </group>
    <group ref="Tabgroup_Other" faims_archent_type="Tabgroup Other">
      <label>{Tabgroup_Other}</label>
      <group ref="Tab">
        <label>{Tab}</label>
        <input faims_attribute_type="measure" ref="Dummy" faims_attribute_name="Dummy">
          <label>{Dummy}</label>
        </input>
      </group>
    </group>
  </h:body>
</h:html>
