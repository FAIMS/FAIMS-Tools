      <bind type="decimal" nodeset="/faims/Epigraphic_record/Object/Colgroup_0/Col_0/Object_Length_Min"/>
      <bind type="decimal" nodeset="/faims/Epigraphic_record/Object/Colgroup_0/Col_0/Object_Width_Min"/>
      <bind type="decimal" nodeset="/faims/Epigraphic_record/Object/Colgroup_0/Col_0/Object_Thickness_Min"/>
      <bind type="decimal" nodeset="/faims/Epigraphic_record/Object/Colgroup_0/Col_1/Object_Length_Max"/>
      <bind type="decimal" nodeset="/faims/Epigraphic_record/Object/Colgroup_0/Col_1/Object_Width_Max"/>
      <bind type="decimal" nodeset="/faims/Epigraphic_record/Object/Colgroup_0/Col_1/Object_Thickness_Max"/>
      <bind type="decimal" nodeset="/faims/Epigraphic_record/Decoration/Number_Figures"/>
