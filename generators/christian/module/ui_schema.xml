      <bind type="decimal" nodeset="/faims/Epigraphic_record/Decoration/Colgroup_0/Col_1/Decor_Height_Max"/>
      <bind type="decimal" nodeset="/faims/Epigraphic_record/Decoration/Colgroup_0/Col_1/Thick_Relief_Max"/>
      <bind type="decimal" nodeset="/faims/Epigraphic_record/Lettering/Number_of_Rows"/>
      <bind type="decimal" nodeset="/faims/Epigraphic_record/Lettering/Number_of_letters"/>
      <bind type="decimal" nodeset="/faims/Epigraphic_record/Lettering/Stoichos_Length"/>
      <bind type="decimal" nodeset="/faims/Epigraphic_record/Lettering/Stoichos_Height"/>
      <bind type="decimal" nodeset="/faims/Epigraphic_record/Interpretation/Start_Year"/>
      <bind type="decimal" nodeset="/faims/Epigraphic_record/Interpretation/End_Year"/>
