<?xml version="1.0"?>
<h:html xmlns:h="http://www.w3.org/1999/xhtml" xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:jr="http://openrosa.org/javarosa" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Fill This In</h:title>
    <model>
      <instance>
        <faims id="Fill_This_In">
          <style>
            <orientation>
              <orientation/>
            </orientation>
            <even>
              <layout_weight/>
            </even>
            <large>
              <layout_weight/>
            </large>
          </style>
          <User>
            <User>
              <Select_User/>
              <Login/>
            </User>
          </User>
          <Control>
            <Main>
              <Take_Feature/>
              <Next_Object_ID/>
              <GPS_Diagnostics/>
            </Main>
            <Search>
              <Colgroup_0>
                <Col_0>
                  <Search_Term/>
                </Col_0>
                <Col_1>
                  <Search_Button/>
                </Col_1>
              </Colgroup_0>
              <Entity_List/>
            </Search>
            <Map>
              <Map/>
            </Map>
          </Control>
          <Burial_Mound>
            <General>
              <Object_ID/>
              <Timestamp/>
              <Author/>
              <Type/>
              <Site_Significance/>
              <Name/>
              <Description/>
              <Source/>
              <Exists_in_Legacy/>
              <Surrounding_Landuse/>
              <Surface_Landuse/>
              <Visibility/>
              <Colgroup_GPS>
                <Col_0>
                  <Latitude/>
                  <Northing/>
                </Col_0>
                <Col_1>
                  <Longitude/>
                  <Easting/>
                </Col_1>
              </Colgroup_GPS>
              <Take_From_GPS/>
              <Elevation/>
              <General_Notes/>
              <Create_Shapefile/>
              <General_Photo/>
              <Button_General_Photo/>
            </General>
            <Dimensions>
              <Profile/>
              <Plan/>
              <Colgroup_0>
                <Col_0>
                  <Length_Max/>
                  <Width_Max/>
                  <Height_Max/>
                  <Area_Max/>
                </Col_0>
                <Col_1>
                  <Length_Min/>
                  <Width_Min/>
                  <Height_Min/>
                  <Area_Min/>
                </Col_1>
              </Colgroup_0>
              <Other_Dimension/>
              <Dimensions_Notes/>
              <Attach_A_Sketch/>
              <Button_Attach_A_Sketch/>
              <Sketch_Photo/>
              <Button_Sketch_Photo/>
            </Dimensions>
            <Material>
              <Stone_Feature_Present_And_Visible/>
              <Stone_Feature_Description/>
              <Surface_Material_Density/>
              <Surface_Material_Description/>
              <Sample_Collected/>
              <Material_Photo/>
              <Button_Material_Photo/>
            </Material>
            <CRM>
              <Disturbance_Kind/>
              <Disturbance_Factors/>
              <Principal_Factors/>
              <Age_of_Damage/>
              <Disturbance_Description/>
              <RT_Kind/>
              <RT_Method/>
              <RT_Frequency/>
              <RT_Description/>
              <Volume_of_Soil_Removed_via_RT/>
              <Affect/>
              <Impact/>
              <Comments_and_Recommendations/>
              <CRM_Photo/>
              <Button_CRM_Photo/>
            </CRM>
          </Burial_Mound>
        </faims>
      </instance>
      <bind type="decimal" nodeset="/faims/Control/Main/Next_Object_ID"/>
      <bind type="decimal" nodeset="/faims/Burial_Mound/General/Object_ID"/>
      <bind type="decimal" nodeset="/faims/Burial_Mound/General/Elevation"/>
    </model>
  </h:head>
  <h:body>
    <group ref="style">
      <label/>
      <group ref="orientation">
        <label/>
        <input ref="orientation">
          <label>horizontal</label>
        </input>
      </group>
      <group ref="even">
        <label/>
        <input ref="layout_weight">
          <label>1</label>
        </input>
      </group>
      <group ref="large">
        <label/>
        <input ref="layout_weight">
          <label>3</label>
        </input>
      </group>
    </group>
    <group ref="User">
      <label>{User}</label>
      <group ref="User">
        <label>{User}</label>
        <select1 ref="Select_User">
          <label>{Select_User}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select1>
        <trigger ref="Login">
          <label>{Login}</label>
        </trigger>
      </group>
    </group>
    <group ref="Control">
      <label>{Control}</label>
      <group ref="Main">
        <label>{Main}</label>
        <trigger ref="Take_Feature">
          <label>{Take_Feature}</label>
        </trigger>
        <input ref="Next_Object_ID" faims_style_class="required">
          <label>{Next_Object_ID}</label>
        </input>
        <input faims_read_only="true" ref="GPS_Diagnostics">
          <label>{GPS_Diagnostics}</label>
        </input>
      </group>
      <group ref="Search" faims_scrollable="false">
        <label>{Records}</label>
        <group faims_style="orientation" ref="Colgroup_0">
          <label/>
          <group faims_style="even" ref="Col_0">
            <label/>
            <input ref="Search_Term">
              <label>{Search_Term}</label>
            </input>
          </group>
          <group faims_style="large" ref="Col_1">
            <label/>
            <trigger ref="Search_Button">
              <label>{Search}</label>
            </trigger>
          </group>
        </group>
        <select1 appearance="compact" ref="Entity_List">
          <label>{Entity_List}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
      </group>
      <group ref="Map" faims_scrollable="false">
        <label>{Map}</label>
        <input faims_map="true" ref="Map">
          <label>{Map}</label>
        </input>
      </group>
    </group>
    <group ref="Burial_Mound" faims_archent_type="Burial Mound">
      <label>{Burial_Mound}</label>
      <group ref="General">
        <label>{General}</label>
        <input faims_attribute_type="measure" ref="Object_ID" faims_attribute_name="Object ID" faims_style_class="required">
          <label>{Object_ID}</label>
        </input>
        <input ref="Timestamp" faims_read_only="true" faims_annotation="false" faims_certainty="false">
          <label>{timestamp}</label>
        </input>
        <input ref="Author" faims_read_only="true" faims_annotation="false" faims_certainty="false">
          <label>{author}</label>
        </input>
        <select1 faims_attribute_type="vocab" ref="Type" faims_attribute_name="Type">
          <label>{Type}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select1>
        <select1 faims_attribute_type="vocab" ref="Site_Significance" faims_attribute_name="Site Significance">
          <label>{Site_Significance}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select1>
        <input faims_attribute_type="measure" ref="Name" faims_attribute_name="Name">
          <label>{Name}</label>
        </input>
        <input faims_attribute_type="measure" ref="Description" faims_attribute_name="Description">
          <label>{Description}</label>
        </input>
        <select1 faims_attribute_type="vocab" ref="Source" faims_attribute_name="Source">
          <label>{Source}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select1>
        <select faims_attribute_type="vocab" ref="Exists_in_Legacy" faims_attribute_name="Exists in Legacy">
          <label>{Exists_in_Legacy}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select>
        <select1 faims_attribute_type="vocab" ref="Surrounding_Landuse" faims_attribute_name="Surrounding Landuse">
          <label>{Surrounding_Landuse}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select1>
        <select1 faims_attribute_type="vocab" ref="Surface_Landuse" faims_attribute_name="Surface Landuse">
          <label>{Surface_Landuse}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select1>
        <select1 faims_attribute_type="vocab" ref="Visibility" faims_attribute_name="Visibility">
          <label>{Visibility}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select1>
        <group ref="Colgroup_GPS" faims_style="orientation">
          <label/>
          <group ref="Col_0" faims_style="even">
            <label/>
            <input ref="Latitude" faims_attribute_name="Latitude" faims_attribute_type="measure" faims_read_only="true">
              <label>{Latitude}</label>
            </input>
            <input ref="Northing" faims_attribute_name="Northing" faims_attribute_type="measure" faims_read_only="true">
              <label>{Northing}</label>
            </input>
          </group>
          <group ref="Col_1" faims_style="even">
            <label/>
            <input ref="Longitude" faims_attribute_name="Longitude" faims_attribute_type="measure" faims_read_only="true">
              <label>{Longitude}</label>
            </input>
            <input ref="Easting" faims_attribute_name="Easting" faims_attribute_type="measure" faims_read_only="true">
              <label>{Easting}</label>
            </input>
          </group>
        </group>
        <trigger ref="Take_From_GPS">
          <label>{Take_From_GPS}</label>
        </trigger>
        <input faims_attribute_type="measure" ref="Elevation" faims_attribute_name="Elevation">
          <label>{Elevation__MASL_}</label>
        </input>
        <input faims_attribute_type="measure" ref="General_Notes" faims_attribute_name="General Notes">
          <label>{General_Notes}</label>
        </input>
        <trigger ref="Create_Shapefile">
          <label>{Create_Shapefile}</label>
        </trigger>
        <select type="camera" faims_attribute_type="measure" faims_sync="true" ref="General_Photo" faims_attribute_name="General Photo">
          <label>{General_Photo}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select>
        <trigger ref="Button_General_Photo">
          <label>{Button_General_Photo}</label>
        </trigger>
      </group>
      <group ref="Dimensions">
        <label>{Dimensions}</label>
        <select1 type="image" faims_attribute_type="vocab" ref="Profile" faims_attribute_name="Profile">
          <label>{Profile}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select1>
        <select1 type="image" faims_attribute_type="vocab" ref="Plan" faims_attribute_name="Plan">
          <label>{Plan}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select1>
        <group ref="Colgroup_0" faims_style="orientation">
          <label/>
          <group ref="Col_0" faims_style="even">
            <label/>
            <input faims_attribute_type="measure" ref="Length_Max" faims_attribute_name="Length Max">
              <label>{Length_Max}</label>
            </input>
            <input faims_attribute_type="measure" ref="Width_Max" faims_attribute_name="Width Max">
              <label>{Width_Max}</label>
            </input>
            <input faims_attribute_type="measure" ref="Height_Max" faims_attribute_name="Height Max">
              <label>{Height_Max}</label>
            </input>
            <input faims_attribute_type="measure" ref="Area_Max" faims_attribute_name="Area Max">
              <label>{Area_Max}</label>
            </input>
          </group>
          <group ref="Col_1" faims_style="even">
            <label/>
            <input faims_attribute_type="measure" ref="Length_Min" faims_attribute_name="Length Min">
              <label>{Length_Min}</label>
            </input>
            <input faims_attribute_type="measure" ref="Width_Min" faims_attribute_name="Width Min">
              <label>{Width_Min}</label>
            </input>
            <input faims_attribute_type="measure" ref="Height_Min" faims_attribute_name="Height Min">
              <label>{Height_Min}</label>
            </input>
            <input faims_attribute_type="measure" ref="Area_Min" faims_attribute_name="Area Min">
              <label>{Area_Min}</label>
            </input>
          </group>
        </group>
        <input faims_attribute_type="measure" ref="Other_Dimension" faims_attribute_name="Other Dimension">
          <label>{Other_Dimension}</label>
        </input>
        <input faims_attribute_type="measure" ref="Dimensions_Notes" faims_attribute_name="Dimensions Notes">
          <label>{Dimensions_Notes}</label>
        </input>
        <select type="file" faims_attribute_type="measure" faims_sync="true" ref="Attach_A_Sketch" faims_attribute_name="Attach A Sketch">
          <label>{Attach_A_Sketch}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select>
        <trigger ref="Button_Attach_A_Sketch">
          <label>{Button_Attach_A_Sketch}</label>
        </trigger>
        <select type="camera" faims_attribute_type="measure" faims_sync="true" ref="Sketch_Photo" faims_attribute_name="Sketch Photo">
          <label>{Sketch_Photo}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select>
        <trigger ref="Button_Sketch_Photo">
          <label>{Button_Sketch_Photo}</label>
        </trigger>
      </group>
      <group ref="Material">
        <label>{Material}</label>
        <select1 appearance="full" faims_attribute_type="vocab" ref="Stone_Feature_Present_And_Visible" faims_attribute_name="Stone Feature Present And Visible">
          <label>{Stone_Feature_Present_And_Visible_}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select1>
        <input faims_attribute_type="measure" ref="Stone_Feature_Description" faims_attribute_name="Stone Feature Description">
          <label>{Stone_Feature_Description}</label>
        </input>
        <select1 faims_attribute_type="vocab" ref="Surface_Material_Density" faims_attribute_name="Surface Material Density">
          <label>{Surface_Material_Density}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select1>
        <input faims_attribute_type="measure" ref="Surface_Material_Description" faims_attribute_name="Surface Material Description">
          <label>{Surface_Material_Description}</label>
        </input>
        <select1 appearance="full" faims_attribute_type="vocab" ref="Sample_Collected" faims_attribute_name="Sample Collected">
          <label>{Sample_Collected_}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select1>
        <select type="camera" faims_attribute_type="measure" faims_sync="true" ref="Material_Photo" faims_attribute_name="Material Photo">
          <label>{Material_Photo}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select>
        <trigger ref="Button_Material_Photo">
          <label>{Button_Material_Photo}</label>
        </trigger>
      </group>
      <group ref="CRM">
        <label>{CRM}</label>
        <select1 faims_attribute_type="vocab" ref="Disturbance_Kind" faims_attribute_name="Disturbance Kind">
          <label>{Disturbance_Kind}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select1>
        <select faims_attribute_type="vocab" ref="Disturbance_Factors" faims_attribute_name="Disturbance Factors">
          <label>{Disturbance_Factors}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select>
        <select1 faims_attribute_type="vocab" ref="Principal_Factors" faims_attribute_name="Principal Factors">
          <label>{Principal_Factors}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select1>
        <select faims_attribute_type="vocab" ref="Age_of_Damage" faims_attribute_name="Age of Damage">
          <label>{Age_of_Damage}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select>
        <input faims_attribute_type="measure" ref="Disturbance_Description" faims_attribute_name="Disturbance Description">
          <label>{Disturbance_Description}</label>
        </input>
        <select faims_attribute_type="vocab" ref="RT_Kind" faims_attribute_name="RT Kind">
          <label>{RT_Kind}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select>
        <select faims_attribute_type="vocab" ref="RT_Method" faims_attribute_name="RT Method">
          <label>{RT_Method}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select>
        <select1 type="image" faims_attribute_type="vocab" ref="RT_Frequency" faims_attribute_name="RT Frequency">
          <label>{RT_Frequency}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select1>
        <input faims_attribute_type="measure" ref="RT_Description" faims_attribute_name="RT Description">
          <label>{RT_Description}</label>
        </input>
        <select1 faims_attribute_type="vocab" ref="Volume_of_Soil_Removed_via_RT" faims_attribute_name="Volume of Soil Removed via RT">
          <label>{Volume_of_Soil_Removed_via_RT}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select1>
        <select1 faims_attribute_type="vocab" ref="Affect" faims_attribute_name="Affect">
          <label>{Affect}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select1>
        <select1 faims_attribute_type="vocab" ref="Impact" faims_attribute_name="Impact">
          <label>{Impact}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select1>
        <input faims_attribute_type="measure" ref="Comments_and_Recommendations" faims_attribute_name="Comments and Recommendations">
          <label>{Comments_and_Recommendations}</label>
        </input>
        <select type="camera" faims_attribute_type="measure" faims_sync="true" ref="CRM_Photo" faims_attribute_name="CRM Photo">
          <label>{CRM_Photo}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select>
        <trigger ref="Button_CRM_Photo">
          <label>{Button_CRM_Photo}</label>
        </trigger>
      </group>
    </group>
  </h:body>
</h:html>
