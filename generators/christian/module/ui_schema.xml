              <Number_Figures/>
              <Anthropomorphic_Figure/>
              <Animal_Figure/>
              <Inanimate_Figure/>
              <Size_Decor/>
              <Decor_Description/>
              <Colgroup_0>
                <Col_0>
                  <Decor_Length_Max/>
                  <Decor_Width_Max/>
                </Col_0>
                <Col_1>
                  <Decor_Height_Max/>
                  <Thick_Relief_Max/>
                </Col_1>
              </Colgroup_0>
              <Add_External_Records/>
              <Photo_Decoration/>
              <Button_Photo_Decoration/>
              <Sketch_Decoration/>
