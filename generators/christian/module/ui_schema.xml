  <h:head>
    <h:title>Fill This In</h:title>
    <model>
      <instance>
        <faims id="Fill_This_In">
          <style>
            <orientation>
              <orientation/>
            </orientation>
            <even>
              <layout_weight/>
            </even>
            <large>
              <layout_weight/>
            </large>
          </style>
          <User>
            <User>
              <Select_User/>
              <Login/>
            </User>
          </User>
          <Control>
            <Main>
              <New_Epigraphic_record/>
              <New_Diary/>
              <Next_Inscription_ID/>
              <GPS_Diagnostics/>
            </Main>
            <Search>
              <Colgroup_0>
                <Col_0>
                  <Search_Term/>
                </Col_0>
                <Col_1>
                  <Search_Button/>
                </Col_1>
              </Colgroup_0>
              <Entity_Types/>
              <Entity_List/>
            </Search>
            <Map>
              <Map/>
            </Map>
          </Control>
          <Diary>
            <Fieldwork_Diary>
              <Diary_ID/>
              <Timestamp/>
              <Author/>
              <Diary_Text/>
            </Fieldwork_Diary>
          </Diary>
          <Epigraphic_record>
            <Admin>
              <Inscription_ID/>
              <Timestamp/>
              <Author/>
            </Admin>
            <Geography>
              <Colgroup_GPS>
                <Col_0>
                  <Latitude/>
                  <Northing/>
                </Col_0>
                <Col_1>
                  <Longitude/>
                  <Easting/>
                </Col_1>
              </Colgroup_GPS>
              <Take_From_GPS/>
              <Modern_name/>
              <Ancient_name/>
              <Archaeological_Context/>
              <Reuse/>
              <Notes/>
              <Location_Accessibility/>
              <Inscription_Accessibility/>
              <Light_conditions/>
              <Location_Photo/>
              <Button_Location_Photo/>
              <Location_Sketch/>
              <Button_Location_Sketch/>
            </Geography>
            <Object>
              <Material/>
              <Stone/>
              <Metal/>
              <Origin_Material/>
              <Description_Material/>
              <Colour/>
              <Object_Type/>
              <Stele_Type/>
              <Vessel_Type/>
              <Architecture_Type/>
              <Sculpure_Type/>
              <Personalia_Type/>
              <Number_fragments/>
              <Preservation/>
              <Colgroup_0>
                <Col_0>
                  <Object_Length_Min/>
                  <Object_Width_Min/>
                  <Object_Thickness_Min/>
                </Col_0>
                <Col_1>
