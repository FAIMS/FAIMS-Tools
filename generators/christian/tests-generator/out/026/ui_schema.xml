<?xml version="1.0"?>
<h:html xmlns:h="http://www.w3.org/1999/xhtml" xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:jr="http://openrosa.org/javarosa" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Fill This In</h:title>
    <model>
      <instance>
        <faims id="Fill_This_In">
          <style>
            <orientation>
              <orientation/>
            </orientation>
            <even>
              <layout_weight/>
            </even>
            <large>
              <layout_weight/>
            </large>
          </style>
          <Tabgroup>
            <Tab>
              <Dummy/>
              <Colgroup_0>
                <Col_0>
                  <Left_1/>
                  <Left_2/>
                </Col_0>
                <Col_1>
                  <Right_1/>
                </Col_1>
              </Colgroup_0>
            </Tab>
          </Tabgroup>
        </faims>
      </instance>
    </model>
  </h:head>
  <h:body>
    <group ref="style">
      <label/>
      <group ref="orientation">
        <label/>
        <input ref="orientation">
          <label>horizontal</label>
        </input>
      </group>
      <group ref="even">
        <label/>
        <input ref="layout_weight">
          <label>1</label>
        </input>
      </group>
      <group ref="large">
        <label/>
        <input ref="layout_weight">
          <label>3</label>
        </input>
      </group>
    </group>
    <group ref="Tabgroup" faims_archent_type="Tabgroup">
      <label>{Tabgroup}</label>
      <group ref="Tab">
        <label>{Tab}</label>
        <input faims_attribute_type="measure" ref="Dummy" faims_attribute_name="Dummy">
          <label>{Dummy}</label>
        </input>
        <group ref="Colgroup_0" faims_style="orientation">
          <label/>
          <group ref="Col_0" faims_style="even">
            <label/>
            <input faims_attribute_type="measure" ref="Left_1" faims_attribute_name="Left 1">
              <label>{Left_1}</label>
            </input>
            <input faims_attribute_type="measure" ref="Left_2" faims_attribute_name="Left 2">
              <label>{Left_2}</label>
            </input>
          </group>
          <group ref="Col_1" faims_style="even">
            <label/>
            <input faims_attribute_type="measure" ref="Right_1" faims_attribute_name="Right 1">
              <label>{Right_1}</label>
            </input>
          </group>
        </group>
      </group>
    </group>
  </h:body>
</h:html>
