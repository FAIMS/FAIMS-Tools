<?xml version="1.0"?>
<dataSchema>
  <ArchaeologicalElement name="Tabgroup">
    <description/>
    <property name="Dummy" isIdentifier="true" type="measure">
      <description/>
      <formatString><![CDATA[{{if $1 then "$1 " else "$2 "}}]]></formatString>
      <appendCharacterString><![CDATA[ - ]]></appendCharacterString>
    </property>
    <property name="Input" type="measure">
      <description/>
    </property>
    <property name="Audio" type="file" file="true" thumbnail="true">
      <description/>
    </property>
    <property name="Button">
      <description/>
    </property>
    <property name="Camera" type="file" file="true" thumbnail="true">
      <description/>
    </property>
    <property name="Checkbox" type="vocab">
      <!--WARNING: A set of <opts> tags are expected but not present-->
      <description/>
    </property>
    <property name="Dropdown" type="vocab">
      <description/>
      <lookup>
        <term>{test_opt}
<description/></term>
      </lookup>
    </property>
    <property name="File" type="file" file="true" thumbnail="true">
      <description/>
    </property>
    <property name="Picture" type="vocab">
      <description/>
      <lookup>
        <term pictureURL="files/data/.">{test_opt}
<description/></term>
      </lookup>
    </property>
    <property name="Radio" type="vocab">
      <description/>
      <lookup>
        <term>{test_opt}
<description/></term>
      </lookup>
    </property>
    <property name="Video" type="file" file="true" thumbnail="true">
      <description/>
    </property>
    <property name="Webview 1">
      <description/>
    </property>
    <property name="Webview 2">
      <description/>
    </property>
  </ArchaeologicalElement>
</dataSchema>
