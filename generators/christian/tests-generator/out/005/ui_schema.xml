<?xml version="1.0"?>
<h:html xmlns:h="http://www.w3.org/1999/xhtml" xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:jr="http://openrosa.org/javarosa" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Fill This In</h:title>
    <model>
      <instance>
        <faims id="Fill_This_In">
          <style>
            <orientation>
              <orientation/>
            </orientation>
            <even>
              <layout_weight/>
            </even>
            <large>
              <layout_weight/>
            </large>
          </style>
          <Tabgroup_1>
            <Tab>
              <Dummy/>
              <Input/>
            </Tab>
          </Tabgroup_1>
          <Tabgroup_2>
            <Tab>
              <Input/>
            </Tab>
          </Tabgroup_2>
        </faims>
      </instance>
    </model>
  </h:head>
  <h:body>
    <group ref="style">
      <label/>
      <group ref="orientation">
        <label/>
        <input ref="orientation">
          <label>horizontal</label>
        </input>
      </group>
      <group ref="even">
        <label/>
        <input ref="layout_weight">
          <label>1</label>
        </input>
      </group>
      <group ref="large">
        <label/>
        <input ref="layout_weight">
          <label>3</label>
        </input>
      </group>
    </group>
    <group ref="Tabgroup_1" faims_archent_type="Tabgroup 1">
      <label>{Tabgroup_1}</label>
      <group ref="Tab">
        <label>{Tab}</label>
        <input faims_attribute_type="measure" ref="Dummy" faims_attribute_name="Dummy">
          <label>{Dummy}</label>
        </input>
        <input faims_attribute_type="measure" ref="Input" faims_attribute_name="Input">
          <label>{Input}</label>
        </input>
      </group>
    </group>
    <group ref="Tabgroup_2" faims_archent_type="Tabgroup 2">
      <label>{Tabgroup_2}</label>
      <group ref="Tab">
        <label>{Tab}</label>
        <input faims_attribute_type="measure" ref="Input" faims_attribute_name="Input">
          <label>{Input}</label>
        </input>
      </group>
    </group>
  </h:body>
</h:html>
