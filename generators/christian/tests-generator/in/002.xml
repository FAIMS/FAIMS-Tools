<?xml version="1.0" ?>
<!--@TEST: GUI elements of all possible types in one tab-->
<module>
  <Tabgroup>
    <Tab>
      <Dummy t="input" f="id"/> <!--f="id" is required to prevent errors-->

      <Audio       t="audio"/>
      <Button      t="trigger"/>
      <Checkbox    t="checkbox"/>
      <Dropdown    t="dropdown"><opts><opt>test opt</opt></opts></Dropdown>
      <File        t="file"/>
      <GPS_Diag    t="gpsdiag"/>
      <Group       t="group"/>
      <List        t="list"><opts><opt>test opt</opt></opts></List>
      <Map         t="map"/>
      <Picture     t="picture"><opts><opt>test opt</opt></opts></Picture>
      <Radio       t="radio"><opts><opt>test opt</opt></opts></Radio>
      <Viewfiles   t="viewfiles"/>
      <Webview_1   t="web"/>
      <Webview_2   t="webview"/>
    </Tab>
  </Tabgroup>
</module>
