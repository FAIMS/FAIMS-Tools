<?xml version="1.0"?>
<h:html xmlns:h="http://www.w3.org/1999/xhtml" xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:jr="http://openrosa.org/javarosa" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Fill This In</h:title>
    <model>
      <instance>
        <faims id="Fill_This_In">
          <style>
            <orientation>
              <orientation/>
            </orientation>
            <even>
              <layout_weight/>
            </even>
            <large>
              <layout_weight/>
            </large>
          </style>
          <Control>
            <Main>
              <Take_Feature/>
              <Object_Starting_ID/>
              <GPS_Diagnostics/>
            </Main>
            <Records>
              <Colgroup_0>
                <Col_0>
                  <Search_Term/>
                </Col_0>
                <Col_1>
                  <Search/>
                </Col_1>
              </Colgroup_0>
              <Results/>
            </Records>
            <Map>
              <Map/>
            </Map>
          </Control>
          <Burial_Mound>
            <General>
              <Object_ID/>
              <Timestamp/>
              <Author/>
              <Type/>
            </General>
          </Burial_Mound>
        </faims>
      </instance>
    </model>
  </h:head>
  <h:body>
    <group ref="style">
      <label/>
      <group ref="orientation">
        <label/>
        <input ref="orientation">
          <label>horizontal</label>
        </input>
      </group>
      <group ref="even">
        <label/>
        <input ref="layout_weight">
          <label>1</label>
        </input>
      </group>
      <group ref="large">
        <label/>
        <input ref="layout_weight">
          <label>3</label>
        </input>
      </group>
    </group>
    <group ref="Control" faims_archent_type="Control">
      <label>{Control}</label>
      <group ref="Main">
        <label>{Main}</label>
        <trigger ref="Take_Feature">
          <label>{Take_Feature}</label>
        </trigger>
        <input ref="Object_Starting_ID">
          <label>{Object_Starting_ID}</label>
        </input>
        <input ref="GPS_Diagnostics">
          <label>{GPS_Diagnostics}</label>
        </input>
      </group>
      <group ref="Records">
        <label>{Records}</label>
        <group ref="Colgroup_0" faims_style="orientation">
          <label/>
          <group ref="Col_0" faims_style="even">
            <label/>
            <input ref="Search_Term">
              <label>{Search_Term}</label>
            </input>
          </group>
          <group ref="Col_1" faims_style="even">
            <label/>
            <trigger ref="Search">
              <label>{Search}</label>
            </trigger>
          </group>
        </group>
        <select1 appearance="compact" ref="Results">
          <label>{Results}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select1>
      </group>
      <group ref="Map" faims_scrollable="false">
        <label>{Map}</label>
        <input faims_map="true" ref="Map">
          <label>{Map}</label>
        </input>
      </group>
    </group>
    <group ref="Burial_Mound" faims_archent_type="Burial_Mound">
      <label>{Burial_Mound}</label>
      <group ref="General">
        <label>{General}</label>
        <input faims_attribute_type="measure" ref="Object_ID" faims_attribute_name="Object ID">
          <label>{Object_ID}</label>
        </input>
        <input ref="Timestamp">
          <label>{Timestamp}</label>
        </input>
        <input ref="Author">
          <label>{Author}</label>
        </input>
        <select1 faims_attribute_type="vocab" ref="Type" faims_attribute_name="Type">
          <label>{Type}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select1>
      </group>
    </group>
  </h:body>
</h:html>
